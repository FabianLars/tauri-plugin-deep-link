{
    "package": {
        "productName": "deep-link-example",
        "version": "0.1.0"
    },
    "build": {
        "distDir": "../public",
        "devPath": "../public"
    },
    "tauri": {
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "de.FabianLars.deep-link-test",
            "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
        },
        "updater": {
            "active": false
        },
        "allowlist": {
            "all": false
        },
        "windows": [
            {
                "title": "app",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": "default-src 'self'"
        }
    }
}
